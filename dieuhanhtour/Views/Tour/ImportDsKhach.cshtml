@model FileUploadViewModel

<script src="~/Asset/js/Tour/DsKhachControl.js"></script>

<div class="row">
    <div class="col-md-12">

        <div class="box">
            <div class="box-header">
                <h4 class="modal-title text-uppercase">NHẬP DS KHÁCH </h4>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool dongkhach"><i class="fa fa-remove"></i></button>
                </div>
            </div>

            <div class="box-body">
                <form id="frmNhapDsKhach"  enctype="multipart/form-data" method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>                    
                                      
                    <fieldset class="groupbox-border">
                        <legend class="groupbox-border">File danh sách khách</legend>
                        <div class="form-group col-md-4">
                            <label class="control-label">Upload file</label>
                            <input  class="form-control" type="file" id="filedskh" multiple />
                            
                        </div>
                       
                    </fieldset>
                    
                </form>
            </div>
            <div class="box-footer col-md-12">
                <input type="button" class="btn btn-primary" id="btImport" data-id="@Model.sgtcode" value="Đồng ý" />
                <button type="button" class="btn btn-default dongkhach">Huỷ</button>
            </div>

        </div>

    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

}
